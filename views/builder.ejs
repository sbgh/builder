                        "icon" : "fa fa-code"
                    },
                    "rerunnable" : {
                        "icon" : "fa fa-repeat rerunnableRow",
                        "a_attr": { class : " rerunnableRow "}
                    },
                    "disabled" : {
                        "icon" : "fa fa-hand-paper-o disabledRow",
