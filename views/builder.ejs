                    "job" : {
                        "icon" : "fa fa-code jobRow"
                    },
                    "needfull" : {
                        "icon" : "fa fa-play-circle-o",
                        "a_attr": { class : " needfull "},
                        "li_attr": { class : " needfull "}
                    },
                    "rerunnable" : {
                        "icon" : "fa fa-repeat rerunnableRow",
                        "a_attr": { class : " rerunnableRow "}
